        "vite": "^4.3.9",
        "vitest": "^0.25.3"
    },
    "type": "module",
    "dependencies": {
        "@popperjs/core": "^2.11.8",
        "flowbite": "^1.6.6",
        "graphql": "^16.6.0",
        "graphql-request": "^6.1.0",
        "svelte-select": "^5.6.1",
        "svelte-switch-case": "^2.0.0",
        "tailwind-merge": "^1.13.2"
    }
